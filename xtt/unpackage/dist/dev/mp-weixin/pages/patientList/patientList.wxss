.bubble {
	position: relative;
	overflow: hidden;
}
.bubble:after {
	content: "44444";
	background: #5c9899;
	position: absolute;
	width: 750rpx;
	height: 750rpx;
	left: calc(50% - 375rpx);
	top: calc(50% - 375rpx);
	opacity: 0;
	margin: auto;
	border-radius: 50%;
	-webkit-transform: scale(1);
	        transform: scale(1);
	transition: all 0.5s ease-in-out;
}

