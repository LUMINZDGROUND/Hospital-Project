<view class="content"><view class="welcomeTitle">设置选项</view><block wx:for="{{titleData}}" wx:for-item="item" wx:for-index="index"><view class="iptView"><view class="title">{{item.title}}</view><input focus="{{true}}" placeholder="{{item.placehold}}" data-event-opts="{{[['input',[['__set_model',['$0','$1','$event',[]],['configData',[['titleData','',index,'vmod']]]]]]]}}" value="{{configData[item.vmod]}}" bindinput="__e"/></view></block><button type="primary" data-event-opts="{{[['tap',[['saveData',['$event']]]]]}}" bindtap="__e">保存</button><button class="btnNext" type="warn" data-event-opts="{{[['tap',[['removeData',['$event']]]]]}}" bindtap="__e">清除缓存</button><button class="btnNext backToLogin" type="default" data-event-opts="{{[['tap',[['backToLogin',['$event']]]]]}}" bindtap="__e">返回</button></view>